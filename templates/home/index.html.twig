{% extends 'base.html.twig' %}

{% block title %}She Starts Africa{% endblock %}

{% block body %}
<div class="main-wrapper home-sst">
    {{ include('home/_header.html.twig')}}
         <div class="slider-area banner-style-3 bg-image">
         <img class="img-ban" data-sal-delay="150" data-sal="fade" data-sal-duration="800"
                                     src="{{ asset('assets/images/banner/banner.png') }}" alt="Banner Images" />
            <div class="d-flex align-items-center height-940">
                <div class="container-ssh sst-animated-shape">
                    <div class="row g-5 align-items-center">
                        <div class="col-lg-6">
                            <div class="inner">
                                <div class="content text-start">
                                    <h1 class="title" data-sal-delay="150" data-sal="slide-up" data-sal-duration="800">By <span class="under">women</span>,</h1>
                                    <h1 class="title two" data-sal-delay="250" data-sal="slide-up" data-sal-duration="800">for <span class="under">women</span></h1>

                                    <p class="description" data-sal-delay="350" data-sal="slide-up" 
                                    data-sal-duration="800">She Starts Africa is a non-for-profit that’s mission is to advance women & increase the number of women actively engaged in the economy through entrepreneurship and leadership.</p>
                                    <a class="edu-btn" href="/discover" data-sal-delay="450" data-sal="slide-up" 
                                    data-sal-duration="800">Know  more about us <i class="icon-arrow-right-line-right"></i></a>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-6">
                            <div class="banner-image">
                                <div class="banner-main-image">
                                    
                                </div>
                            </div>
                        </div>
                    </div>
                    
                </div>
            </div>
        </div>
  <div class="about">
    <div class="row">
            <div class="col-md-6" style="    margin-top: 60px;">
            <span class="pre-title" data-sal-delay="150" data-sal="slide-up" data-sal-duration="800">The Female Founders Program</span>
            <p class="about-desc"> 
This program targets female-led businesses based across Africa, in order to help them re-invent their business model, enhance their business capabilities, use no code platforms as a first step in digital transformation, and start the business growth process.<p>
            <a class="about-btn" href="/about" data-sal-delay="450" data-sal="slide-up" 
                                        data-sal-duration="800">Learn more </a>
            </div>
            <div class="col-md-6">
                <img class="about-femme" src="{{ asset('assets/images/banner/femme.png') }}" alt="Banner Images">
            </div>
    </div>
  </div>
<div class="container">
    <div class="row">
          <div class="col-md-3 text-center">
          <div class="service-card shape-bg-3">
            <div class="inner">
                <div class="icon">
                      <i class="icon-checkbox-circle-fill"></i>
                </div>
                <div class="content">
                    <h6 class="title">Safe space</h6>
                </div>
            </div>
            </div>
          </div>
           <div class="col-md-3 text-center">
            <div class="service-card shape-bg-3">
            <div class="inner">
                <div class="icon">
                      <i class="icon-checkbox-circle-fill"></i>
                </div>
                <div class="content">
                    <h6 class="title">Matchmaking</h6>
                </div>
            </div>
            </div>
            </div>
          <div class="col-md-3 text-center">
           <div class="service-card shape-bg-3">
            <div class="inner">
                <div class="icon">
                     <i class="icon-checkbox-circle-fill"></i>
                </div>
                <div class="content">
                    <h6 class="title">Networking</h6>
                </div>
            </div>
            </div>
            </div>
           <div class="col-md-3 text-center">
            <div class="service-card shape-bg-3">
            <div class="inner">
                <div class="icon">
                      <i class="icon-checkbox-circle-fill "></i>
                </div>
                <div class="content">
                    <h6 class="title">Roadshow</h6>
                </div>
            </div>
            </div>
        </div>     
    </div>
</div>

<div class="edu-blog-area sst-home-one-blog">
        <div class="shape-dot-wrapper shape-wrapper d-xl-block d-none">
            <div class="shape-image shape-image-4">
                <img class="turn-img" src="assets/images/shapes/shape-16-01.png" alt="Shape Thumb"> 
            </div>
        </div>
    <div class="container-sst">
        <div class="row g-5 align-items-center">
            <div class="col-lg-6 col-md-6 col-12">
                <div class="section-title text-start" data-sal-delay="150" data-sal="slide-up" data-sal-duration="800">
                    <span class="pre-titre">Our Ventures</span>
                    <h3 class="title">Latest Ventures</h3>
                </div>
            </div>
        </div>

        <div class="row g-5">
        <div class="eedu-testimonial-area sst-home-two-testimonial testimonial-card-box-bg edu-section-gap position-relative bg-image">
                <div class="edu-testimonial-activation testimonial-item-3 mt--40 edu-slick-button">
        {% for venture in ventures %}
        
            <!-- Start Blog Grid  -->
            <div class="col-12 col-sm-12 col-xl-4 col-md-6 sal-animate" data-sal-delay="150" data-sal="slide-up" data-sal-duration="800">   
			    <div class="sst-ventures edu-blog blog-type-2 bg-whiterss radius-small ">
                    <div class="inner">
                        <div class="content">
                            <div class="thumbnail">
                               
                                  <img class="vent-img" src="{{ asset('assets/images/ventures/')}}{{ venture.cover }}" alt="ventures Images">
                               
                            </div>
                            <h4 class="title-vent">
                            {{ venture.title }}</h4>
                            <div class="blog-card-bottom">
                                <p class="desc-venture">
                                {{ venture.description|slice(0, 160) ~ '...' }}
                                </p>
                                
                            </div>
                            <div class="card-bottom row">
                                <ul class="edu-meta meta-01">
                                            <li><img class="icon icon-vent" src="{{ asset('assets/images/icons/tag.png') }}">
												{{ venture.sector }}</li>
                                            <li><img class="icon icon-vent" src="{{ asset('assets/images/icons/symbol.png') }}">{{ venture.country }}</li>
                                        </ul>
                                        <ul class="edu-meta meta-01">
                                            <li><img class="icon icon-vent" src="{{ asset('assets/images/icons/calendar.png') }}">
												{{ venture.years|date('Y') }}</li>
                                            <li><img class="icon icon-vent" src="{{ asset('assets/images/icons/group.png') }}">{{ venture.founder }} Founders</li>
                                        </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- End Blog Grid  -->
           {% endfor %}
        </div>
        </div>
        </div>
    </div>
</div>
    <!-- Start Testimonial Area  -->
   
    <div class="container-sst sst-animated-shape mt--130">
    
        <div class="row  align-items-center testimo">
                    <div class="section-title testim text-start " >
                            <span class="testim-title">Testimonials</span>
                           <h3 class="title-testim">Our female founders feedback</h3>
                   </div>
                    <div class="sst-testimonial-three inner sst-home-five-testimonial-activation slick-arrow-style-2 edu-slick-button slick-button-left">
                    {% for testim in testimonials %}
                                <!-- Start Tastimonial Card  -->
                            <div class="testimonial-card-box variation-3">
                                 <div class="row" >
                                     <div class="part1 col-md-8">
                                         <p class="description">“ {{ testim.description }} </p>
                                     </div>
                                     <div class="part2 col-md-4">
                                        <img class="testim-img" src="{{ asset('assets/images/testimonials/')}}{{ testim.image }}">
                                       <div class="bloc-name"><p class="name-testim">{{ testim.name }} </p></div>
                                      </div>
                                </div>
                            </div>
                                <!-- End Tastimonial Card  -->
                    {% endfor %}
                    </div>
            </div>
    </div>
    <!-- End Testimonial Area  -->

 {{ include('home/_partenaires.html.twig')}} 
 {{ include('home/_footer.html.twig')}}
 <style>
 img.vent-img {
    width: 100%;
    height: 200px;
}.edu-blog.blog-type-2 {
    overflow: hidden;
    position: relative;
    transition: 0.4s;
    margin: 15px;
}
 </style>
{% endblock %}
